<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
  <head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="shortcut icon" href="data:image/x-icon;base64," />

    <!-- <title>Suomen kirjastot – Kirjastohakemisto</title> -->
    <title>{% block title %}Welcome!{% endblock %} – Kirjastohakemisto</title>

    <noscript>
      <style>
        blockquote {
          margin: 0;
          font-style: italic;
        }

        address h3 {
          font-style: normal;
        }

        footer {
          margin-top: 15px;
          margin: 0;
        }
      </style>
    </noscript>
    <script defer data-domain="hakemisto.kirjastot.fi" type="text/plain" data-kifi-consent="analytics" data-src="https://plausible.io/js/script.js"></script>
    {% if app.request.locale == 'sv' %}
      <script src="https://cookiehub.net/c2/0540ebb9.js"></script>
    {% elseif app.request.locale == 'en' %}
      <script src="https://cookiehub.net/c2/e3d60465.js"></script>
    {% else %}
      <script src="https://cookiehub.net/c2/b5d438d6.js"></script>
    {% endif %}
    <script src="https://gfx.kirjastot.fi/kifi/consent/csp-detect-blocked.js"></script>
    <script src="https://gfx.kirjastot.fi/kifi/consent/cookiehub.js"></script>
  </head>
  <body id="page--frontpage">
    {% block body %}{% endblock %}
  </body>
</html>
